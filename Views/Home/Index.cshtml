@using NewModelEx_S1.Models

@{
    
    ViewBag.Title = "Index";
}
@{
    var parameter = Request.QueryString["v"];

    if (parameter == "1")
    {



        <script>alert("Record Successfully.");</script>



    }

}
<ol class="breadcrumb " style="background-color :transparent;font-size:11px;">
    <li class="breadcrumb-item" active" aria-current="page">Home</li>
</ol>

 
<div class="col-lg-12">
    <div class="row">
        <div class="col-lg-9">

            <a href="@Url.Action("AddNewModel", "Home" )"><i class='fas fa-plus' style="color:#0094ff"></i> </a>

        </div>
        <div class="col-lg-3">

            <input type="text" name="name" value="" id="myInput" placeholder="search" class="form-control form-control-sm " />

        </div>

    </div>
   
</div>

    <div class="col-lg-12">




        <div class="table-responsive">


            <table class="table table-bordered table-sm table-hover" style="font-size:14px;margin-top:5px;" id="myTable">
                <thead class="thead-dark">

                    <tr>
                        <th>Program Name</th>
                        <th>Customer Name</th>
                        <th align="center">Drawing#</th>
                        <th>Model Name</th>
                        <th>Part Name</th>

                        <th>Date</th>
                        <th>Classification</th>
                        <th>Other Info</th>
                        <th colspan="3"></th>

                    </tr>

                </thead>

                @foreach (ModelNew modelnew in (ModelNew[])ViewData["showmodelnew"])
                {
                    <tr style="font-size:12px;">
                        <td>@modelnew.ProgramName</td>
                        <td>@modelnew.CustomerName</td>
                        <td align="center">@modelnew.ModelDrawing</td>
                        <td>@modelnew.ModelName</td>
                        <td>@modelnew.PartName</td>

                        <td>@modelnew.ModelDate</td>
                        <td>@modelnew.Classification</td>
                        <td>@modelnew.OtherInfo</td>
                        <td align="center">
                            <a href="@Url.Action("ModelDetial", "Home", new { id=@modelnew.ModelDrawing })"><i class='fas fa-pen' style="color:#000000"></i></a>
                        </td>
                        <td align="center">
                            <a href="@Url.Action("EditNewModel", "Home", new { id = @modelnew.ModelDrawing })"><i class='fas fa-wrench' style="color:#000000"></i></a>
                        </td>
                        <td align="center">

                            <i class='fas fa-trash' style="color:#000000" onclick="Checkdel(this);"></i>
                        </td>


                    </tr>
                }
            </table>
        </div>

    </div>
